{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist && npm run postbuild:spa",
    "typecheck": "tsc -b",
    "postbuild:spa": "copyfiles -u 3 ../deploy/ovh/.htaccess dist/",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --coverage",
    "test:component": "cypress run --component",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "test:lighthouse": "lhci autorun",
    "test:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "test:all": "npm run test:unit && npm run test:component && npm run test:e2e && npm run test:lighthouse",
    "type-check": "tsc --noEmit",
    "doctor": "npm run type-check && npm run lint && npm run test:all",
    "clean": "rm -rf dist node_modules/.vite coverage cypress/videos cypress/screenshots lighthouse-reports"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.87.4",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^24.4.0",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.1.1",
    "dexie": "^4.2.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/vite-dev-server": "^7.0.0",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "cypress": "^15.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "lighthouse-ci": "^1.13.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
